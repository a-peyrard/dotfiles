# ========================================
# DEVSERVER OVERRIDE - Green/Blue Theme
# ========================================
set -g status-style bg=default,fg=colour39
set-window-option -g window-status-style fg=colour39,bg=default
set-window-option -g window-status-format ' #I#[fg=colour240]:#[fg=colour250]#W#F '
set-window-option -g window-status-current-style fg=colour46,bg=default,bold
set-window-option -g window-status-current-format ' #I#[fg=colour240]:#[fg=colour114]#W '
set-option -g pane-border-style fg=colour22,bg=default
set-option -g pane-active-border-style bold,fg=colour46,bg=default
set-option -g display-panes-active-colour colour46
set-option -g display-panes-colour colour39
set-window-option -g clock-mode-colour colour46
