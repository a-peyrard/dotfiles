# Sapling (sl) command aliases - matching git aliases
alias ss="sl status"
alias sd="sl diff"
alias sdc="sl diff --rev ."  # Show changes in current commit (like git diff --cached)
alias slog="sl ssl"  # Smartlog - shows relevant commits in a graph
alias samend="sl amend"  # Amend current commit
alias srebase="sl rebase"
alias sshow="sl show"  # Show current commit
